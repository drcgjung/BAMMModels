@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#>.
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#>.
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:2.0.0#>.
@prefix unit: <urn:bamm:io.openmanufacturing:unit:2.0.0#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <urn:bamm:io.catenax.fleet.claim_data:1.0.0#>.

:ClaimData a bamm:Aspect;
    bamm:preferredName "ClaimData"@en;
    bamm:description "Claim data from a fleet"@en;
    bamm:properties (:listOfClaims);
    bamm:operations ();
    bamm:events ().
:listOfClaims a bamm:Property;
    bamm:preferredName "listOfClaims"@en;
    bamm:description "List of Claims"@en;
    bamm:characteristic :ListOfClaims.
:ListOfClaims a bamm-c:List;
    bamm:preferredName "ListOfClaims"@en;
    bamm:description "List of Claims"@en;
    bamm:dataType :Claim.
:Claim a bamm:Entity;
    bamm:preferredName "Text"@en;
    bamm:description "everything to describe a claim"@en;
    bamm:properties (:listOfDiagnosticSessionId :repairMileage :repairDate :technicianComment :customerComment :claimId :vehicleIdentifiers :listOfParts :qTaskId :damageCode).
:listOfDiagnosticSessionId a bamm:Property;
    bamm:preferredName "ListOfDiagnosticSessionId"@en;
    bamm:description "references to a list of diagnostic session IDs"@en;
    bamm:characteristic :Text.
:repairMileage a bamm:Property;
    bamm:preferredName "Repair mileage"@en;
    bamm:description "mileage of the car when the claim was reported"@en;
    bamm:characteristic :Mileage;
    bamm:exampleValue "10251"^^xsd:positiveInteger.
:repairDate a bamm:Property;
    bamm:preferredName "RepairDate"@en;
    bamm:description "references the date when claim was initial reported"@en;
    bamm:characteristic bamm-c:Timestamp;
    bamm:exampleValue "2022-02-04T14:48:54"^^xsd:dateTime.
:technicianComment a bamm:Property;
    bamm:preferredName "TechniciansComment"@de-de;
    bamm:description "short description of the claim from the technicians"@en;
    bamm:characteristic bamm-c:MultiLanguageText.
:customerComment a bamm:Property;
    bamm:preferredName "CustomerComment"@en;
    bamm:description "short description of the claim from customer view"@en;
    bamm:characteristic bamm-c:MultiLanguageText.
:claimId a bamm:Property;
    bamm:preferredName "ClaimID"@en;
    bamm:description "Claim ID: Unique for each OEM"@en;
    bamm:characteristic :Text.
:vehicleIdentifiers a bamm:Property;
    bamm:preferredName "vehicle identifiers"@en;
    bamm:description "one single vehicle"@en;
    bamm:characteristic :VehicleIdentifiersCharacteristic.
:listOfParts a bamm:Property;
    bamm:preferredName "ListOfParts"@en;
    bamm:description "a list of parts which will be replaced or repaired"@en;
    bamm:characteristic :ListOfPartsCharacteristic.
:qTaskId a bamm:Property;
    bamm:preferredName "QTaskID"@en;
    bamm:description "Reference to qTask: A unique identifier. The company creating this quality task sets this identifer. The identifier should contain the BPN to make it unique inside CX network"@en;
    bamm:characteristic :Text.
:damageCode a bamm:Property;
    bamm:preferredName "DamageCode"@en, "Schadensschluessel"@de-de;
    bamm:description "OEM-specific damage code"@en;
    bamm:characteristic :Text.
:Text a bamm:Characteristic;
    bamm:description "Describes a Property which contains plain text. This is intended exclusively for human readable strings, not for identifiers, measurement values, etc."@en;
    bamm:dataType :DiagnosticSessionId.
:Mileage a bamm-c:Measurement;
    bamm:preferredName "Mileage"@en;
    bamm:description "mileage of the car when claim was reported"@de-de;
    bamm:dataType xsd:positiveInteger;
    bamm-c:unit unit:kilometre.
:VehicleIdentifiersCharacteristic a bamm:Characteristic;
    bamm:preferredName "Vehicle"@en;
    bamm:description "one single vehicle"@en;
    bamm:dataType :VehicleIdentifiers.
:ListOfPartsCharacteristic a bamm-c:SingleEntity;
    bamm:preferredName "ListOfPartsCharacteristic"@en;
    bamm:description "a list of parts"@en;
    bamm:dataType :Part.
:DiagnosticSessionId a bamm:Entity;
    bamm:preferredName "DiagnosticSessionID"@en;
    bamm:description "one specific diagnostic session ID"@en;
    bamm:properties ().
:VehicleIdentifiers a bamm:Entity;
    bamm:preferredName "Vehicle identifiers"@en;
    bamm:description "one single vehicle"@en;
    bamm:properties (:vehicleCatenaXId :anonymizedId).
:Part a bamm:Entity;
    bamm:preferredName "Part"@en;
    bamm:description "one specific part"@en;
    bamm:properties (:replacedPartCatenaXId :partReplaced :replacedPartNumber :replacedPartName :replacedPartSerialNumber :partCausal :amountOfReplacedParts :supplierId :sparePartCatenaXId :sparePartNumber :sparePartSerialNumber :sparePartName).
:vehicleCatenaXId a bamm:Property;
    bamm:preferredName "vehicleCatenaXId"@en;
    bamm:description "Catena-X car ID /digital twin of car"@en;
    bamm:characteristic :Text.
:anonymizedId a bamm:Property;
    bamm:preferredName "AnonymizedID"@en;
    bamm:description "link to diagnostic data set created for this claim"@en;
    bamm:characteristic :Text.
:replacedPartCatenaXId a bamm:Property;
    bamm:preferredName "ReplacedPartCatenaXId"@en;
    bamm:description "the ID of the affected part"@en;
    bamm:characteristic :Text.
:partReplaced a bamm:Property;
    bamm:preferredName "PartReplaced"@en;
    bamm:description "flag is set if part was replaced\nTrue: replaced\nFalse: not replaced"@en;
    bamm:characteristic bamm-c:Boolean;
    bamm:exampleValue "1"^^xsd:boolean.
:replacedPartNumber a bamm:Property;
    bamm:preferredName "ReplacedPartNumber"@en;
    bamm:description "OEM part number of the part that was put out the vehicle"@en;
    bamm:characteristic :Text.
:replacedPartName a bamm:Property;
    bamm:preferredName "ReplacedPartName"@en;
    bamm:description "OEM part name of the part that was put out the vehicle"@en;
    bamm:characteristic :Text.
:replacedPartSerialNumber a bamm:Property;
    bamm:preferredName "ReplacedPartSerialNumber"@en;
    bamm:description "OEM serial part number of the part that was put out the vehicle - only available for serial parts"@en;
    bamm:characteristic :Text.
:partCausal a bamm:Property;
    bamm:preferredName "PartCausal"@en;
    bamm:description "flag set to True if part was causing the problem\nTrue: part caused the problem\nFalse: part didn't cause the problem\n"@en;
    bamm:characteristic bamm-c:Boolean;
    bamm:exampleValue "True"^^xsd:boolean.
:amountOfReplacedParts a bamm:Property;
    bamm:preferredName "AmountOfReplacedParts"@en;
    bamm:description "counter for non-serial parts"@en;
    bamm:characteristic :Text.
:supplierId a bamm:Property;
    bamm:preferredName "SupplierId"@en;
    bamm:description "OEM-specific ID of the supplier that manufactured the part that was put out - available if known"@en;
    bamm:characteristic :Text.
:sparePartCatenaXId a bamm:Property;
    bamm:preferredName "SparePartCatenaXId"@en;
    bamm:description "CX digital twin ID of the part that was put in"@en;
    bamm:characteristic :Text.
:sparePartNumber a bamm:Property;
    bamm:preferredName "SparePartNumber"@en;
    bamm:description "OEM part number of the part that was put in the vehicle"@en;
    bamm:characteristic :Text.
:sparePartSerialNumber a bamm:Property;
    bamm:preferredName "SparePartSerialNumber"@en;
    bamm:description "OEM serial part number of the part that was put in the vehicle - only available for serial parts"@en;
    bamm:characteristic :Text.
:sparePartName a bamm:Property;
    bamm:preferredName "SparePartName"@en;
    bamm:description "OEM part name of the part that was put in the vehicle"@en;
    bamm:characteristic :Text.
