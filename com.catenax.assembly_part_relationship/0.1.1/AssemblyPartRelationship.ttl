@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:1.0.0#>.
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:1.0.0#>.
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:1.0.0#>.
@prefix unit: <urn:bamm:io.openmanufacturing:unit:1.0.0#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <urn:bamm:com.catenax.assembly_part_relationship:0.1.1#>.

:AssemblyPartRelationship a bamm:Aspect;
    bamm:name "AssemblyPartRelationship";
    bamm:preferredName "Assembly Part Relationship"@en;
    bamm:properties (:catenaXId :childParts);
    bamm:description "The aspect provides the information which parts the object assembles."@en;
    bamm:operations ().
:catenaXId a bamm:Property;
    bamm:name "catenaXId";
    bamm:preferredName "Catena-X Identifier"@en;
    bamm:description "The Catena-X ID of the given parent Part (e.g. the Assembly), valid for the Catena-X dataspace."@en;
    bamm:characteristic :CatenaXUniqueIDTrait;
    bamm:exampleValue "055c1128-0375-47c8-98de-7cf802c3241d".
:childParts a bamm:Property;
    bamm:name "childParts";
    bamm:preferredName "Child Parts"@en;
    bamm:description "List of Catena-X IDs  and (meta-) data of assembled child parts"@en;
    bamm:characteristic :SetofChildPartsCharacteristic.
:CatenaXUniqueIDTrait a bamm-c:Trait;
    bamm:name "CatenaXUniqueIDTrait";
    bamm:description "Trait to ensure UUID v4 data format"@en;
    bamm-c:baseCharacteristic :UUIDv4Characteristic;
    bamm-c:constraint :UUIDv4RegularExpression.
:UUIDv4Characteristic a bamm:Characteristic;
    bamm:name "UUIDv4Characteristic";
    bamm:preferredName "UUID v4"@en;
    bamm:description "A UUID is a 128-bit unique value expressed as string. UUID Version 4 is used to generate a Unique ID based on random number generation. "@en;
    bamm:dataType xsd:string.
:UUIDv4RegularExpression a bamm-c:RegularExpressionConstraint;
    bamm:name "UUIDv4RegularExpression";
    bamm:preferredName "Catena-X UniqueId Regular Expression"@en;
    bamm:description "Regular Expression for UUID v4"@en;
    bamm:value "^[0-9a-fA-F]{8}\\\\b-[0-9a-fA-F]{4}\\\\b-[0-9a-fA-F]{4}\\\\b-[0-9a-fA-F]{4}\\\\b-[0-9a-fA-F]{12}$".
:ChildData a bamm:Entity;
    bamm:name "ChildData";
    bamm:preferredName "Child Object Data"@en;
    bamm:properties (:createdOn :quantity [
  bamm:property :lastModifiedOn;
  bamm:optional "true"^^xsd:boolean
] :context :childCatenaXId);
    bamm:description "set of physical/digital child objects data by which the parent object is assembled by."@en.
:createdOn a bamm:Property;
    bamm:name "createdOn";
    bamm:preferredName "Creation Date"@en;
    bamm:description "Timestamp when the relation between parent and child was created"@en;
    bamm:characteristic bamm-c:Timestamp;
    bamm:exampleValue "2022-02-03T14:48:54.709Z"^^xsd:dateTime.
:quantity a bamm:Property;
    bamm:name "quantity";
    bamm:preferredName "Quantity"@en;
    bamm:description "Quantity of child parts. \"1\" for serialized parts"@en;
    bamm:characteristic :QuantityCharacteristic;
    bamm:exampleValue "1 litre"^^:Quantity.
:lastModifiedOn a bamm:Property;
    bamm:name "lastModifiedOn";
    bamm:preferredName "Last Modification Date"@en;
    bamm:description "Timestamp when the relation between parent and child was last modified"@en;
    bamm:characteristic bamm-c:Timestamp;
    bamm:exampleValue "2022-02-03T14:48:54.709Z"^^xsd:dateTime.
:context a bamm:Property;
    bamm:name "context";
    bamm:description "The lifecycle context of the Assembly-Part relationship"@en;
    bamm:characteristic :UsageCharacteristic;
    bamm:exampleValue "as-built".
:childCatenaXId a bamm:Property;
    bamm:name "childCatenaXId";
    bamm:preferredName "Catena-X Child Identifiers "@en;
    bamm:description "The Catena-X IDs of the child objects the part is assembled by."@en;
    bamm:characteristic :CatenaXUniqueIDTrait.
:QuantityCharacteristic a bamm-c:Quantifiable;
    bamm:name "QuantityCharacteristic";
    bamm:description "The quantity in which the child part is assembled in the given object."@en;
    bamm:dataType :Quantity.
:UsageCharacteristic a bamm-c:Enumeration;
    bamm:name "UsageCharacteristic";
    bamm:preferredName "Usage"@en;
    bamm:description "as-required, as-designed, as-planned, as-built, as-maintained, as-recycled"@en;
    bamm:dataType xsd:string;
    bamm-c:values ("as-required" "as-designed" "as-planned" "as-built" "as-maintained" "as-recycled").
:SetofChildPartsCharacteristic a bamm-c:Set;
    bamm:name "SetofChildPartsCharacteristic";
    bamm:preferredName "Set of Child Parts"@en;
    bamm:description "Physical and digital child parts the parent objects is assembled by (one structural level down)."@en;
    bamm:dataType :ChildData.
:quantityNumber a bamm:Property;
    bamm:name "quantityNumber";
    bamm:description "The number of objects related to the measurement unit"@en;
    bamm:characteristic :NumberofObjects;
    bamm:exampleValue "2.5"^^xsd:double.
:measurementUnit a bamm:Property;
    bamm:name "measurementUnit";
    bamm:description "Unit of Measurement for the quantity of serialized objects"@en;
    bamm:characteristic bamm-c:UnitReference;
    bamm:exampleValue "litre"^^bamm:curie.
:NumberofObjects a bamm:Characteristic;
    bamm:name "NumberofObjects";
    bamm:description "Quantifiable number of objects in reference to the measurementUnit"@en;
    bamm:dataType xsd:double.
:Quantity a bamm:Entity;
    bamm:name "Quantity";
    bamm:properties (:quantityNumber :measurementUnit);
    bamm:description "Comprises the number of objects and the unit of measurement for the respective child objects"@en.
