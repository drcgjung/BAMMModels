@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:1.0.0#>.
@prefix unit: <urn:bamm:io.openmanufacturing:unit:1.0.0#>.
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:1.0.0#>.
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:1.0.0#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix : <urn:bamm:com.catenax:0.1.1#>.

:ProductUsageVehicle a bamm:Aspect;
    bamm:name "ProductUsageVehicle";
    bamm:properties (:mileage :equipmentvariants :KBA :firstRegistration);
    bamm:operations ();
    bamm:preferredName "Product Usage of Vehicle"@en.
:mileage a bamm:Property;
    bamm:name "mileage";
    bamm:characteristic :Mileage;
    bamm:description "The mileage stated by the vehicle"@en.
:Mileage a bamm-c:Quantifiable;
    bamm:name "Mileage";
    bamm:dataType xsd:double;
    bamm-c:unit unit:kilometre.
:equipmentvariants a bamm:Property;
    bamm:name "equipmentvariants";
    bamm:characteristic :EquipmentCharacteristic.
:EquipmentCharacteristic a bamm:Characteristic;
    bamm:name "EquipmentCharacteristic";
    bamm:dataType :VariantEntity.
:VariantEntity a bamm:Entity;
    bamm:name "VariantEntity";
    bamm:properties (:variantID);
    bamm:operations ();
    bamm:preferredName "Equipment Variants"@en.
:variantID a bamm:Property;
    bamm:name "variantID";
    bamm:description "The equipment variants codes"@en;
    bamm:characteristic :Variant.
:Variant a bamm:Characteristic;
    bamm:name "Variant";
    bamm:dataType xsd:string.
:KBA a bamm:Property;
    bamm:name "KBA";
    bamm:description "The equipment variants codes"@en;
    bamm:characteristic :Identifier.
:Identifier a bamm:Characteristic;
    bamm:name "Identifier";
    bamm:dataType xsd:string.
:firstRegistration a bamm:Property;
    bamm:name "firstRegistration";
    bamm:description "the first registration of the vehicle in GMT time zone"@en;
    bamm:exampleValue "2021-09-30"^^xsd:dateTime;
    bamm:characteristic bamm-c:Timestamp.