@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:1.0.0#>.
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:1.0.0#>.
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:1.0.0#>.
@prefix unit: <urn:bamm:io.openmanufacturing:unit:1.0.0#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <urn:bamm:io.catenax.pcf:2.0.0#>.

:Pcf a bamm:Aspect;
    bamm:name "Pcf";
    bamm:properties (:productFootprintId :pcf :productFootprintSpecVersion :productFootprintVersion [
  bamm:property :productFootprintUpdated;
  bamm:optional "true"^^xsd:boolean
] :companyName :companyIds :productDescription :productIds :productCategoryCpc :productNameCompany :productFootprintComment);
    bamm:operations ();
    bamm:events ();
    bamm:preferredName "PCF (Product Carbon Footprint)"@en;
    bamm:description "Normative: Each Product (Carbon) Footprint represents the carbon footprint of a product with values in accordance with the Pathfinder Framework.\nNon-normative: Each Product Footprint relates to a specific product, identified by one or more product identifiers. The Product Footprint is modeled as a multi purpose container for product-specific factors."@en;
    bamm:see <https://wbcsd.github.io/introduction/>.
:productFootprintId a bamm:Property;
    bamm:name "productFootprintId";
    bamm:preferredName "Product Footprint Identifier"@en;
    bamm:description "The product footprint identifier."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :ProductFootprintIdTrait;
    bamm:exampleValue "3893bb5d-da16-4dc1-9185-11d97476c254".
:pcf a bamm:Property;
    bamm:name "pcf";
    bamm:preferredName "PCF (Product Carbon Footprint)"@en;
    bamm:description "A PCF (Product Carbon Footprint) represents the carbon footprint of a product and related data in accordance with the Pathfinder Framework."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :PcfCharacteristic.
:productFootprintSpecVersion a bamm:Property;
    bamm:name "productFootprintSpecVersion";
    bamm:preferredName "Product Footprint Specification Version"@en;
    bamm:description "Version of the ProductFootprint data specification. The value of specVersion MUST be \"1.0.0\".\nNote: subsequent revisions of the ProductFootprint data specification will update this value according to the rules of Semantic Versioning 2.0.0."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :ProductFootprintSpecVersionTrait;
    bamm:exampleValue "1.0.0".
:productFootprintVersion a bamm:Property;
    bamm:name "productFootprintVersion";
    bamm:preferredName "ProductFootprintVersion"@en;
    bamm:description "Whenever a data owner or a host system updates a Product Footprint it MUST set the version to be by strictly greater than the value of all preceding footprints."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :ProductFootprintVersionCharacteristic;
    bamm:exampleValue "42"^^xsd:positiveInteger.
:productFootprintUpdated a bamm:Property;
    bamm:name "productFootprintUpdated";
    bamm:preferredName "Updated"@en;
    bamm:description "Product Footprint Updated"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :ProductFootprintUpdatedCharacteristic;
    bamm:exampleValue "2022-05-22T21:47:35Z"^^xsd:dateTime.
:companyName a bamm:Property;
    bamm:name "companyName";
    bamm:preferredName "Company Name"@en;
    bamm:description "Name of the company that is the ProductFootprint Data Owner."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :CompanyNameCharacteristic;
    bamm:exampleValue "Company A".
:companyIds a bamm:Property;
    bamm:name "companyIds";
    bamm:preferredName "Company IDs (Identifications)"@en;
    bamm:description "CompanyIds (Identifications) with value the non-empty set of Uniform Resource Names (URN)2. Each value of this set is supposed to uniquely identify the ProductFootprint Data Owner."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :CompanyIdsCharacteristic;
    bamm:exampleValue "URN:foo:a123,456".
:productDescription a bamm:Property;
    bamm:name "productDescription";
    bamm:preferredName "Product Description"@en;
    bamm:description "Free-form description of the product plus other information related to it such as production technology or packaging."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic bamm-c:Text;
    bamm:exampleValue "Car X model B type C".
:productIds a bamm:Property;
    bamm:name "productIds";
    bamm:preferredName "Product IDs (Identifications)"@en;
    bamm:description "Each of the values in the set is supposed to uniquely identify the product. "@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :ProductIdsCharacteristic;
    bamm:exampleValue "urn:ISBN:3-8273-7019-1".
:productCategoryCpc a bamm:Property;
    bamm:name "productCategoryCpc";
    bamm:preferredName "Product Category "@en;
    bamm:description "Central Product Classification Code (CPC) that the given product belongs to."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :ProductCategoryCpcCharacteristic;
    bamm:exampleValue "31230".
:productNameCompany a bamm:Property;
    bamm:name "productNameCompany";
    bamm:preferredName "Product Trade Name"@en;
    bamm:description "Trade name of the product."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic bamm-c:Text;
    bamm:exampleValue "Car X".
:productFootprintComment a bamm:Property;
    bamm:name "productFootprintComment";
    bamm:preferredName "Comment"@en;
    bamm:description "Additional information and instructions related to the calculation of the footprint, or other information which informs the ability to interpret, to audit or to verify the Product Footprint."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic bamm-c:Text;
    bamm:exampleValue "Comment for Version 42".
:ProductFootprintIdTrait a bamm-c:Trait;
    bamm:name "ProductFootprintIdTrait";
    bamm-c:baseCharacteristic bamm-c:Text;
    bamm-c:constraint :ProductFootprintIdConstraint.
:PcfCharacteristic a bamm-c:SingleEntity;
    bamm:name "PcfCharacteristic";
    bamm:preferredName "PCF (Product Carbon Footprint) Characteristic"@en;
    bamm:description "The \"pcf\" (product carbon footprint) property contains several attributes/ properties as specified by WBCSD (World Business Council for Sustainable Development)."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType :PcfEntity.
:ProductFootprintSpecVersionTrait a bamm-c:Trait;
    bamm:name "ProductFootprintSpecVersionTrait";
    bamm-c:baseCharacteristic :ProductFootprintSpecVersionCharacteristic;
    bamm-c:constraint :ProductFootprintSpecificatonVersioConstraint.
:ProductFootprintVersionCharacteristic a bamm:Characteristic;
    bamm:name "ProductFootprintVersionCharacteristic";
    bamm:preferredName "Product Footprint Version Characteristic"@en;
    bamm:description "Product Footprint Version Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:positiveInteger.
:ProductFootprintUpdatedCharacteristic a bamm:Characteristic;
    bamm:name "ProductFootprintUpdatedCharacteristic";
    bamm:preferredName "Product Footprint Updated Characteristic"@en;
    bamm:description "Product Footprint Updated Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:dateTime.
:CompanyNameCharacteristic a bamm:Characteristic;
    bamm:name "CompanyNameCharacteristic";
    bamm:preferredName "Company Name Characteristic"@en;
    bamm:description "Company Name Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:string.
:CompanyIdsCharacteristic a bamm-c:Set;
    bamm:name "CompanyIdsCharacteristic";
    bamm:preferredName "Company Ids (Identifications) Characteristic"@en;
    bamm:description "Company Ids (Identifications) Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:string.
:ProductIdsCharacteristic a bamm-c:Set;
    bamm:name "ProductIdsCharacteristic";
    bamm:preferredName "Product Ids (Identifications) Characteristic"@en;
    bamm:description "Product Ids (Identifications) Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:string.
:ProductCategoryCpcCharacteristic a bamm:Characteristic;
    bamm:name "ProductCategoryCpcCharacteristic";
    bamm:preferredName "Product Category Cpc  (Central Product Classification) Characteristic"@en;
    bamm:description "Product Category Cpc  (Central Product Classification) Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:string.
:ProductFootprintIdConstraint a bamm-c:RegularExpressionConstraint;
    bamm:name "ProductFootprintIdConstraint";
    bamm:preferredName "Product Footprint Identifier Constraint"@en;
    bamm:description "Constraint for defining a Product Footprint Identifier. Conform to Catena-X this needs to be an UUID (universally unique identifier) v4 format."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:value "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$".
:PcfEntity a bamm:Entity;
    bamm:name "PcfEntity";
    bamm:properties (:declaredUnit :unitaryProductAmount :created :validityPeriodEnd :reportingPeriodStart :reportingPeriodEnd [
  bamm:property :geographyCountrySubdivision;
  bamm:optional "true"^^xsd:boolean
] [
  bamm:property :geographyCountry;
  bamm:optional "true"^^xsd:boolean
] [
  bamm:property :geographyRegionOrSubregion;
  bamm:optional "true"^^xsd:boolean
] :primaryDataShare [
  bamm:property :emissionFactorSources;
  bamm:optional "true"^^xsd:boolean
] [
  bamm:property :boundaryProcessesDescription;
  bamm:optional "true"^^xsd:boolean
] :boundaryGate :crossSectoralStandardsUsed :productOrSectorSpecificRules :fossilGhgEmissions :biogenicCarbonContent [
  bamm:property :biogenicEmissions;
  bamm:optional "true"^^xsd:boolean
]);
    bamm:preferredName "PCF Entity"@en;
    bamm:description "Entity for defining the properties of a pcf as specified by WBCSD."@en.
:ProductFootprintSpecVersionCharacteristic a bamm:Characteristic;
    bamm:name "ProductFootprintSpecVersionCharacteristic";
    bamm:preferredName "Product Footprint Specification Version Characteristic"@en;
    bamm:description "Product Footprint Specification Version Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:string.
:ProductFootprintSpecificatonVersioConstraint a bamm-c:RegularExpressionConstraint;
    bamm:name "ProductFootprintSpecificatonVersioConstraint";
    bamm:preferredName "Product Footprint Specificaton Versio Constraint"@en;
    bamm:description "Constraint for defining a Product Footprint Specification"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:value "\\d+\\.\\d+\\.\\d+".
:declaredUnit a bamm:Property;
    bamm:name "declaredUnit";
    bamm:preferredName "Unit of measurement"@en;
    bamm:description "Unit of analysis of the product"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic bamm-c:UnitReference.
:unitaryProductAmount a bamm:Property;
    bamm:name "unitaryProductAmount";
    bamm:preferredName "Package size of referred product"@en;
    bamm:description "Amount of units contained within the product the PCF (Product Carbon Footprint) is referring to."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :UnitaryProductAmountTrait;
    bamm:exampleValue "1.5"^^xsd:float.
:created a bamm:Property;
    bamm:name "created";
    bamm:preferredName "Validity Start"@en;
    bamm:description "Timestamp of the creation of the Product Carbon Footprint."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :CreatedCharacteristic;
    bamm:exampleValue "2022-08-31T00:00:00Z"^^xsd:dateTime.
:validityPeriodEnd a bamm:Property;
    bamm:name "validityPeriodEnd";
    bamm:preferredName "Validity End"@en;
    bamm:description "End of the period during which the data shown are valid."@en;
    bamm:characteristic :ValidityPeriodEndCharacteristic;
    bamm:exampleValue "2032-12-31T00:00:00Z"^^xsd:dateTime.
:reportingPeriodStart a bamm:Property;
    bamm:name "reportingPeriodStart";
    bamm:preferredName "Reporting Period Start"@en;
    bamm:description "Reporting Period Start"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :ReportingPeriodStartCharacteristic;
    bamm:exampleValue "2021-01-01T00:00:00Z"^^xsd:dateTime.
:reportingPeriodEnd a bamm:Property;
    bamm:name "reportingPeriodEnd";
    bamm:preferredName "Reporting Period End"@en;
    bamm:description "Reporting Period End"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :ReportingPeriodEndCharacteristic;
    bamm:exampleValue "2022-01-01T00:00:00Z"^^xsd:dateTime.
:geographyCountrySubdivision a bamm:Property;
    bamm:name "geographyCountrySubdivision";
    bamm:preferredName "Subdivision"@en;
    bamm:description "Subdivision of a country"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :GeographyCountrySubdivisionTrait;
    bamm:exampleValue "US-NY".
:geographyCountry a bamm:Property;
    bamm:name "geographyCountry";
    bamm:preferredName "Country"@en;
    bamm:description "Two letter country code"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :GeographyCountryTrait;
    bamm:exampleValue "DE".
:geographyRegionOrSubregion a bamm:Property;
    bamm:name "geographyRegionOrSubregion";
    bamm:preferredName "Region"@en;
    bamm:description "Region according to list"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :GeographyRegionOrSubregionCharacteristic;
    bamm:exampleValue "Europe".
:primaryDataShare a bamm:Property;
    bamm:name "primaryDataShare";
    bamm:preferredName "Primary Data Share"@en;
    bamm:description "Share of primary data"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :PrimaryDataShareTrait;
    bamm:exampleValue "80"^^xsd:float.
:emissionFactorSources a bamm:Property;
    bamm:name "emissionFactorSources";
    bamm:preferredName "Emission Factor Data Sources"@en;
    bamm:description "Emission factor databases accepted under Version 1 of the Pathfinder Framework (see the Pathfinder Framework Section 6.2)."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :EmissionFactorSourcesCharacteristic.
:boundaryProcessesDescription a bamm:Property;
    bamm:name "boundaryProcessesDescription";
    bamm:preferredName "Process Description"@en;
    bamm:description "Processes attributable to each lifecycle stage."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic bamm-c:Text.
:boundaryGate a bamm:Property;
    bamm:name "boundaryGate";
    bamm:preferredName "Boundary Gate"@en;
    bamm:description "Definition of PCF (Product Carbon Footprint) system boundary, e.g. \"cradle-to-gate\". This follows the LCA (Life Cycle Assessment) methodology."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :BoundaryGateCharacteristic;
    bamm:exampleValue "cradle-to-gate".
:crossSectoralStandardsUsed a bamm:Property;
    bamm:name "crossSectoralStandardsUsed";
    bamm:preferredName "Cross Sectoral Standards Used"@en;
    bamm:description "Discloses the cross-sectoral standards applied for calculating or allocating GHG (Greenhouse Gas) emissions."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :CrossSectoralStandardSet.
:productOrSectorSpecificRules a bamm:Property;
    bamm:name "productOrSectorSpecificRules";
    bamm:preferredName "Product or Sector Specific Rules"@en;
    bamm:description "Product or sector specific rules applied for calculating or allocating GHG (Greenhouse Gas) emissions, e.g. PCRs (Product Category Rules), including operators/ pubishers and according rule names."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :ProductOrSectorSpecificRulesCharacteristic.
:fossilGhgEmissions a bamm:Property;
    bamm:name "fossilGhgEmissions";
    bamm:preferredName "Fossil Emissions"@en;
    bamm:description "Emissions from the combustion of fossil sources. "@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :FossilGhgEmissionsTrait;
    bamm:exampleValue "80"^^xsd:float.
:biogenicCarbonContent a bamm:Property;
    bamm:name "biogenicCarbonContent";
    bamm:preferredName "Biogenic Carbon Content"@en;
    bamm:description "Mass of biogenic carbon per given unit of exchange."@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :BiogenicCarbonContentCharacteristic;
    bamm:exampleValue "5"^^xsd:float.
:biogenicEmissions a bamm:Property;
    bamm:name "biogenicEmissions";
    bamm:preferredName "Biogenic Emissions"@en;
    bamm:description "Summary of land (change) emissions and other"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:characteristic :BiogenicEmissionsCharacteristic.
:UnitaryProductAmountTrait a bamm-c:Trait;
    bamm:name "UnitaryProductAmountTrait";
    bamm-c:baseCharacteristic :UnitaryProductAmountCharacteristic;
    bamm-c:constraint :UnitaryProductAmountConstraint.
:CreatedCharacteristic a bamm:Characteristic;
    bamm:name "CreatedCharacteristic";
    bamm:preferredName "Created Characteristic"@en;
    bamm:description "Created Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:dateTime.
:ValidityPeriodEndCharacteristic a bamm:Characteristic;
    bamm:name "ValidityPeriodEndCharacteristic";
    bamm:preferredName "Validity Period End Characteristic"@en;
    bamm:description "Validity Period End Characteristic"@en;
    bamm:dataType xsd:dateTime.
:ReportingPeriodStartCharacteristic a bamm:Characteristic;
    bamm:name "ReportingPeriodStartCharacteristic";
    bamm:preferredName "Reporting Period Start Characteristic"@en;
    bamm:description "Reporting Period Start Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:dateTime.
:ReportingPeriodEndCharacteristic a bamm:Characteristic;
    bamm:name "ReportingPeriodEndCharacteristic";
    bamm:preferredName "Reporting Period End Characteristic"@en;
    bamm:description "Reporting Period End Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:dateTime.
:GeographyCountrySubdivisionTrait a bamm-c:Trait;
    bamm:name "GeographyCountrySubdivisionTrait";
    bamm-c:baseCharacteristic :GeographyCountrySubdivisionCharacteristic;
    bamm-c:constraint :GeographyCountrySubdivisionCharacteristicConstraint.
:GeographyCountryTrait a bamm-c:Trait;
    bamm:name "GeographyCountryTrait";
    bamm-c:baseCharacteristic :GeographyCountryCharacteristic;
    bamm-c:constraint :GeographyCountryConstraint.
:GeographyRegionOrSubregionCharacteristic a bamm-c:Enumeration;
    bamm:name "GeographyRegionOrSubregionCharacteristic";
    bamm:preferredName "Geography Region Or Subregion Characteristic"@en;
    bamm:description "Geography Region Or Subregion Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:string;
    bamm-c:values ("Africa" "Americas" "Asia" "Europe" "Oceania" "Australia and New Zealand" "Central Asia" "Eastern Asia" "Eastern Europe" "Latin America and the Caribbean" "Melanesia" "Micronesia" "Northern Africa" "Northern America" "Northern Europe" "Polynesia" "South-eastern Asia" "Southern Asia" "Southern Europe" "Sub-Saharan Africa" "Western Asia" "Western Europe").
:PrimaryDataShareTrait a bamm-c:Trait;
    bamm:name "PrimaryDataShareTrait";
    bamm-c:baseCharacteristic :PrimaryDataShareCharacteristic;
    bamm-c:constraint :PrimaryDataShareConstraint.
:EmissionFactorSourcesCharacteristic a bamm-c:Set;
    bamm:name "EmissionFactorSourcesCharacteristic";
    bamm:preferredName "Emission Factor Sources Characteristic"@en;
    bamm:description "Emission Factor Sources Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType :EmissionFactorSourcesEntity.
:BoundaryGateCharacteristic a bamm-c:Enumeration;
    bamm:name "BoundaryGateCharacteristic";
    bamm:preferredName "Boundary Gate Characteristic"@en;
    bamm:description "Boundary Gate Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:string;
    bamm-c:values ("cradle-to-gate" "gate-to-gate").
:ProductOrSectorSpecificRulesCharacteristic a bamm-c:SingleEntity;
    bamm:name "ProductOrSectorSpecificRulesCharacteristic";
    bamm:preferredName "Product Or Sector Specific Rules Characteristic"@en;
    bamm:description "Product Or Sector Specific Rules Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType :ProductOrSectorSpecificRulesEntity.
:FossilGhgEmissionsTrait a bamm-c:Trait;
    bamm:name "FossilGhgEmissionsTrait";
    bamm-c:baseCharacteristic :FossilGhgEmissionsCharacteristic;
    bamm-c:constraint :FossilGhgEmissionsConstraint.
:BiogenicCarbonContentCharacteristic a bamm-c:Quantifiable;
    bamm:name "BiogenicCarbonContentCharacteristic";
    bamm:preferredName "Biogenic Carbon Content Characteristic"@en;
    bamm:description "Biogenic Carbon Content Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:float;
    bamm-c:unit :PcfEmissionUnit.
:BiogenicEmissionsCharacteristic a bamm-c:SingleEntity;
    bamm:name "BiogenicEmissionsCharacteristic";
    bamm:preferredName "Biogenic Emissions Characteristic"@en;
    bamm:description "Biogenic Emissions Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType :BiogenicEmissionsEntity.
:UnitaryProductAmountCharacteristic a bamm:Characteristic;
    bamm:name "UnitaryProductAmountCharacteristic";
    bamm:preferredName "Unitary Product Amount Characteristic"@en;
    bamm:description "Characteristic for defining the amount of a product"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:float.
:UnitaryProductAmountConstraint a bamm-c:RangeConstraint;
    bamm:name "UnitaryProductAmountConstraint";
    bamm:preferredName "Unitary Product Amount Constraint"@en;
    bamm:description "Unitary Product Amount Constraint"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm-c:minValue "0"^^xsd:float;
    bamm-c:lowerBoundDefinition bamm-c:GREATER_THAN;
    bamm-c:upperBoundDefinition bamm-c:AT_MOST.
:GeographyCountrySubdivisionCharacteristic a bamm:Characteristic;
    bamm:name "GeographyCountrySubdivisionCharacteristic";
    bamm:preferredName "Geography Country Subdivision Characteristic"@en;
    bamm:description "Geography Country Subdivision Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:string.
:GeographyCountrySubdivisionCharacteristicConstraint a bamm-c:RegularExpressionConstraint;
    bamm:name "GeographyCountrySubdivisionCharacteristicConstraint";
    bamm:preferredName "Geography Country Subdivision Characteristic Constraint"@en;
    bamm:description "Geography Country Subdivision Characteristic Constraint"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:value "([A-Z]{2}-[A-Z0-9]{1,3}|)".
:GeographyCountryCharacteristic a bamm:Characteristic;
    bamm:name "GeographyCountryCharacteristic";
    bamm:preferredName "Geography Country Characteristic"@en;
    bamm:description "Geography Country Characteristic"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:dataType xsd:string.
:GeographyCountryConstraint a bamm-c:RegularExpressionConstraint;
    bamm:name "GeographyCountryConstraint";
    bamm:preferredName "Geography Country Constraint"@en;
    bamm:description "Geography Country Constraint"@en;
    bamm:see <https://wbcsd.github.io/introduction/>;
    bamm:value "([A-Z]{2})".
:PrimaryDataShareCharacteristic a bamm-c:Quantifiable;
