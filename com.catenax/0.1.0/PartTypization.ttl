@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:1.0.0#>.
@prefix unit: <urn:bamm:io.openmanufacturing:unit:1.0.0#>.
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:1.0.0#>.
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:1.0.0#>.
@prefix : <urn:bamm:com.catenaX:0.1.0#>.

:LocalIdentifierCharacteristics a bamm-c:Set;
    bamm:name "LocalIdentifierCharacteristics";
    bamm:dataType :LocalIdentifierEntity;
    bamm:preferredName "Local Identifier Characteristics"@en.
:LocalIdentifierEntity a bamm:Entity;
    bamm:name "LocalIdentifierEntity";
    bamm:preferredName "Local Identifier"@en;
    bamm:description "The local identifier is a set of key value pairs. At least one key value pair must exist."@en;
    bamm:properties (:key :value).
:key a bamm:Property;
    bamm:name "key";
    bamm:preferredName "Identifier Key"@en;
    bamm:description "The key (domain, namespace) component of an identifier."@en;
    bamm:characteristic bamm-c:Text;
    bamm:exampleValue "serial number".
:value a bamm:Property;
    bamm:name "value";
    bamm:preferredName "Identifier Value"@en;
    bamm:description "The value (name) component of an identifier."@en;
    bamm:characteristic bamm-c:Text;
    bamm:exampleValue "SN12345".
:partName a bamm:Property;
    bamm:name "partName";
    bamm:preferredName "partName"@en;
    bamm:description "Name of the part, as defined by the manufacturer"@en;
    bamm:characteristic bamm-c:Text;
    bamm:exampleValue "MIRROR LEFT".
:PartTypization a bamm:Aspect;
    bamm:name "PartTypization";
    bamm:preferredName "Part"@en;
    bamm:description "A part, i.e. a non-serialized object (\"Katalogteil\")"@en;
    bamm:properties (:catenaXUniqueId :localIdentifier :partName [
  bamm:property :partNameCustomer;
  bamm:optional "true"^^xsd:boolean
] :createdOn [
  bamm:property :modifiedOn;
  bamm:optional "true"^^xsd:boolean
] [
  bamm:property :releasedOn;
  bamm:optional "true"^^xsd:boolean
] [
  bamm:property :type;
  bamm:optional "true"^^xsd:boolean
] :version [
  bamm:property :context;
  bamm:optional "true"^^xsd:boolean
] :description);
    bamm:operations ().
:catenaXUniqueId a bamm:Property;
    bamm:name "catenaXUniqueId";
    bamm:preferredName "Catena-X Unique ID"@en;
    bamm:characteristic :CatenaXUniqueIdTrait.
:localIdentifier a bamm:Property;
    bamm:name "localIdentifier";
    bamm:preferredName "local Identifiers"@en;
    bamm:description "The local identifier is valid in the manufacturers/producers dataspace und uniquly identifies a part e.g. via a part number"@en;
    bamm:characteristic :LocalIdentifierCharacteristics;
    bamm:exampleValue "key: \"com.t-systems.windchill.serialnumber\", value: \"SN12345\""^^:LocalIdentifierEntity.
:partNameCustomer a bamm:Property;
    bamm:name "partNameCustomer";
    bamm:preferredName "Part Name Customer"@en;
    bamm:description "Name of the part, as assigned by the customer of the part"@en;
    bamm:characteristic bamm-c:Text;
    bamm:exampleValue "Mirror left".
:createdOn a bamm:Property;
    bamm:name "createdOn";
    bamm:preferredName "Creation Date"@en;
    bamm:characteristic :DateWithoutTime;
    bamm:exampleValue "2021-12-15"^^xsd:date.
:modifiedOn a bamm:Property;
    bamm:name "modifiedOn";
    bamm:preferredName "Modification Date"@en;
    bamm:characteristic :DateWithoutTime;
    bamm:exampleValue "2021-12-16"^^xsd:date.
:releasedOn a bamm:Property;
    bamm:name "releasedOn";
    bamm:preferredName "Release Date"@en;
    bamm:characteristic :DateWithoutTime;
    bamm:exampleValue "2021-12-17"^^xsd:date.
:type a bamm:Property;
    bamm:name "type";
    bamm:preferredName "Type"@en;
    bamm:characteristic :Type;
    bamm:exampleValue "piece part".
:version a bamm:Property;
    bamm:name "version";
    bamm:preferredName "Version"@en;
    bamm:description "Version represents the identification of a specific version of the base Part."@en;
    bamm:characteristic :Version.
:description a bamm:Property;
    bamm:name "description";
    bamm:preferredName "Description"@en;
    bamm:description "An expanded name or text that provides further information about the\nPart."@en;
    bamm:characteristic :Description.
:CatenaXUniqueIdTrait a bamm-c:Trait;
    bamm:name "CatenaXUniqueIdTrait";
    bamm:preferredName "Catena-X UniqueID Trait"@en;
    bamm-c:baseCharacteristic :UUIDv4;
    bamm-c:constraint :UUIDv4RegEx.
:DateWithoutTime a bamm-c:TimeSeries;
    bamm:name "DateWithoutTime";
    bamm:dataType xsd:date.
:Version a bamm:Characteristic;
    bamm:name "Version";
    bamm:dataType xsd:string;
    bamm:preferredName "Version"@en.
:Description a bamm:Characteristic;
    bamm:name "Description";
    bamm:dataType xsd:string;
    bamm:preferredName "Description"@en.
:UUIDv4 a bamm:Characteristic;
    bamm:name "UUIDv4";
    bamm:dataType xsd:string;
    bamm:preferredName "UUID v4"@en;
    bamm:description "UUID v4 are generated randomly and with no inherent logic."@en.
:UUIDv4RegEx a bamm-c:RegularExpressionConstraint;
    bamm:name "UUIDv4RegEx";
    bamm:preferredName "UUIDv4 Regular Expression"@en;
    bamm:description "Regular Expression for UUID v4"@en;
    bamm:value "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$".
:Type a bamm-c:Enumeration;
    bamm:name "Type";
    bamm:dataType xsd:string;
    bamm:preferredName "Type"@en;
    bamm-c:values ("Assembly" "Component" "Software" "Tool" "Raw Material" "Product" "Piece Part").
:context a bamm:Property;
    bamm:name "context";
    bamm:preferredName "Context"@en;
    bamm:characteristic :Context;
    bamm:exampleValue "as-designed".
:Context a bamm-c:Enumeration;
    bamm:name "Context";
    bamm:dataType xsd:string;
    bamm:preferredName "Context"@en;
    bamm:description "The lifecycle context of the part"@en;
    bamm-c:values ("as-designed" "as-planned" "as-built" "as-maintained" "as-recycled").
