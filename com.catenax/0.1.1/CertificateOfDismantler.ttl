@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:1.0.0#>.
@prefix unit: <urn:bamm:io.openmanufacturing:unit:1.0.0#>.
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:1.0.0#>.
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:1.0.0#>.
@prefix catenax-c: <urn:bamm:com.catenaX.addressCharacteristic:0.1.1#>.
@prefix : <urn:bamm:com.catenaX:0.1.1#>.

:dismantlingCompany a bamm:Property;
    bamm:name "dismantlingCompany";
    bamm:characteristic :Dismantling;
    bamm:preferredName "dismantling company"@en;
    bamm:description "data for the dismantling company"@en.
:Dismantling a bamm-c:SingleEntity;
    bamm:name "Dismantling";
    bamm:dataType :Dismantler.
:Dismantler a bamm:Entity;
    bamm:name "Dismantler";
    bamm:properties (:dismantlerAdress :licensedBy :licensedByPublicAuthority :contactInformation).
:dismantlerAdress a bamm:Property;
    bamm:name "dismantlerAdress";
    bamm:preferredName "dismantler adress"@en;
    bamm:characteristic catenax-c:PostalAdress.
:licensedBy a bamm:Property;
    bamm:name "licensedBy";
    bamm:characteristic :Advisor;
    bamm:preferredName "licensed by"@en.
:licensedByPublicAuthority a bamm:Property;
    bamm:name "licensedByPublicAuthority";
    bamm:characteristic :Authority;
    bamm:preferredName "licensed by public authority"@en.
:contactInformation a bamm:Property;
    bamm:name "contactInformation";
    bamm:characteristic :ContactInformation;
    bamm:preferredName "contact information"@en;
    bamm:description "the contact information via phone or fax"@en.
:Advisor a bamm:Characteristic;
    bamm:name "Advisor";
    bamm:dataType :LegalPerson;
    bamm:preferredName "Advisor"@en.
:Authority a bamm-c:SingleEntity;
    bamm:name "Authority";
    bamm:dataType :GovernmentOffice;
    bamm:preferredName "Authority"@en.
:ContactInformation a bamm-c:SingleEntity;
    bamm:name "ContactInformation";
    bamm:dataType :Contact.
:LegalPerson a bamm:Entity;
    bamm:name "LegalPerson";
    bamm:properties (:contactInformation :advisorAdress).
:GovernmentOffice a bamm:Entity;
    bamm:name "GovernmentOffice";
    bamm:properties (:governmentAdress);
    bamm:preferredName "GovernmentOffice"@en.
:Contact a bamm:Entity;
    bamm:name "Contact";
    bamm:properties ([
  bamm:property :fax;
  bamm:optional "true"^^xsd:boolean
] :phoneNumber).
:Number a bamm:Characteristic;
    bamm:name "Number";
    bamm:dataType xsd:float;
    bamm:preferredName "Number"@en.
:advisorAdress a bamm:Property;
    bamm:name "advisorAdress";
    bamm:preferredName "advisor adress"@en;
    bamm:description "the adress of the advisor or inspector"@en;
    bamm:characteristic catenax-c:PostalAdress.
:governmentAdress a bamm:Property;
    bamm:name "governmentAdress";
    bamm:preferredName "government adress"@en;
    bamm:description "the adress of the public government"@en;
    bamm:characteristic catenax-c:PostalAdress.
:phoneNumber a bamm:Property;
    bamm:name "phoneNumber";
    bamm:characteristic :Number;
    bamm:preferredName "phoneNumber"@en;
    bamm:exampleValue "01774639523"^^xsd:float.
:fax a bamm:Property;
    bamm:name "fax";
    bamm:characteristic :Number;
    bamm:preferredName "fax"@en;
    bamm:exampleValue "05690234585"^^xsd:float.
:CertificateOfDismantler a bamm:Aspect;
    bamm:name "CertificateOfDismantler";
    bamm:properties (:dismantlingCompany :licenseNumber :issueDate :expiryDate :erzeugernummer :befoerderernummer :entsorgernummer);
    bamm:preferredName "CertificateOfDismantler"@en;
    bamm:operations ().
:licenseNumber a bamm:Property;
    bamm:name "licenseNumber";
    bamm:characteristic :Identification;
    bamm:preferredName "license number"@en;
    bamm:exampleValue "HAL-FG123".
:issueDate a bamm:Property;
    bamm:name "issueDate";
    bamm:characteristic bamm-c:Timestamp;
    bamm:preferredName "issue date"@en;
    bamm:exampleValue "2021-08-20"^^xsd:dateTime.
:expiryDate a bamm:Property;
    bamm:name "expiryDate";
    bamm:characteristic bamm-c:Timestamp;
    bamm:preferredName "expiry date"@en;
    bamm:exampleValue "2022-08-20"^^xsd:dateTime.
:erzeugernummer a bamm:Property;
    bamm:name "erzeugernummer";
    bamm:characteristic :Identification;
    bamm:preferredName "erzeugernummer"@en;
    bamm:exampleValue "S30E00529".
:befoerderernummer a bamm:Property;
    bamm:name "befoerderernummer";
    bamm:characteristic :Identification;
    bamm:preferredName "befoerderernummer"@en;
    bamm:exampleValue "S30T00026".
:entsorgernummer a bamm:Property;
    bamm:name "entsorgernummer";
    bamm:characteristic :Identification;
    bamm:preferredName "entsorgernummer"@en;
    bamm:exampleValue "S30A00065".
:Identification a bamm:Characteristic;
    bamm:name "Identification";
    bamm:dataType xsd:string;
    bamm:preferredName "Identification"@en.
