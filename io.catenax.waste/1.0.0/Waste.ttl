@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:1.0.0#>.
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:1.0.0#>.
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:1.0.0#>.
@prefix unit: <urn:bamm:io.openmanufacturing:unit:1.0.0#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <urn:bamm:io.openmanufacturing.digitaltwin:1.0.0#>.

:Characteristic1 a bamm:Characteristic;
    bamm:name "Characteristic1";
    bamm:dataType :Wastecode.
:Waste a bamm:Aspect;
    bamm:name "Waste";
    bamm:properties (:wasteCode :wasteCodeDescription :metalScrapIdentificationCode :productComposition :productPurity :contamination :marketIndexMaterialPrice :marketIndexCurrency :formFactor :density :odor :phValue :hazardousness :packagingType);
    bamm:operations ();
    bamm:events ();
    bamm:preferredName "waste"@en;
    bamm:description "Waste materials which occurs during dissasembly (tbd)."@en.
:Characteristic2 a bamm:Characteristic;
    bamm:name "Characteristic2";
    bamm:dataType :Wastecode.
:wasteCode a bamm:Property;
    bamm:name "wasteCode";
    bamm:characteristic :Characteristic1;
    bamm:preferredName "Waste code"@en;
    bamm:description "The waste code. According to: https://www.gesetze-im-internet.de/avv/anlage.html"@en;
    bamm:exampleValue "02 01 10"^^:Wastecode.
:wasteCodeDescription a bamm:Property;
    bamm:name "wasteCodeDescription";
    bamm:characteristic :Characteristic2;
    bamm:preferredName "Waste Code Description"@en;
    bamm:description "The waste code description. According to: https://www.gesetze-im-internet.de/avv/anlage.html"@en;
    bamm:exampleValue "Metallabfälle"^^:Wastecode.
:metalScrapIdentificationCode a bamm:Property;
    bamm:name "metalScrapIdentificationCode";
    bamm:characteristic :Characteristic3;
    bamm:preferredName "Metal Scrap Identification Code"@en;
    bamm:description "The scrap identification code. Example: https://bdsv.de/de/resources/aktuelle%20Stahlschrottsortenliste%20Deutschland_nL.pdf"@en;
    bamm:exampleValue "47 - Geshredderter oder durch ähnliche Verfahren aufbereiteter Stahlschrott aus der Müllseparation".
:productComposition a bamm:Property;
    bamm:name "productComposition";
    bamm:characteristic :Characteristic4;
    bamm:preferredName "Product Composition"@en;
    bamm:description "Material composition."@en;
    bamm:exampleValue "Plastics 30%, Cobalt 30%, Copper 12%".
:Characteristic3 a bamm:Characteristic;
    bamm:name "Characteristic3";
    bamm:dataType xsd:string.
:Characteristic4 a bamm:Characteristic;
    bamm:name "Characteristic4";
    bamm:dataType xsd:string.
:productPurity a bamm:Property;
    bamm:name "productPurity";
    bamm:characteristic :Characteristic5;
    bamm:preferredName "Product Purity"@en;
    bamm:description "Material purity"@en;
    bamm:exampleValue "98% aluminium".
:Characteristic5 a bamm:Characteristic;
    bamm:name "Characteristic5";
    bamm:dataType xsd:string.
:contamination a bamm:Property;
    bamm:name "contamination";
    bamm:characteristic :Characteristic6;
    bamm:preferredName "Contamination"@en;
    bamm:description "Possible contamination, e.g. with oil"@en;
    bamm:exampleValue "Light oil contamination, Stark verschmutzt".
:Characteristic6 a bamm:Characteristic;
    bamm:name "Characteristic6";
    bamm:dataType xsd:string.
:marketIndexMaterialPrice a bamm:Property;
    bamm:name "marketIndexMaterialPrice";
    bamm:characteristic :Characteristic7;
    bamm:preferredName "Market Index Material Price"@en;
    bamm:description "The market index for a material predominating material composition of a product (relevant for production waste/scrap). Example: https://www.bdsv.org/unser-service/markt-preise/"@en;
    bamm:exampleValue "300"^^xsd:double.
:Characteristic7 a bamm:Characteristic;
    bamm:name "Characteristic7";
    bamm:dataType xsd:double.
:marketIndexCurrency a bamm:Property;
    bamm:name "marketIndexCurrency";
    bamm:characteristic :Characteristic8;
    bamm:preferredName "Market Index Currency"@en;
    bamm:description "The currency in which market index is calculated. According to: ISO 4217"@en;
    bamm:exampleValue "EUR".
:Characteristic8 a bamm:Characteristic;
    bamm:name "Characteristic8";
    bamm:dataType xsd:string.
:formFactor a bamm:Property;
    bamm:name "formFactor";
    bamm:characteristic :Characteristic9;
    bamm:preferredName "Form Factor"@en;
    bamm:description "The product form factor."@en;
    bamm:exampleValue "Aluminum sheets Gepressed, gestapelt, rund, eckig".
:Characteristic9 a bamm:Characteristic;
    bamm:name "Characteristic9";
    bamm:dataType xsd:string.
:density a bamm:Property;
    bamm:name "density";
    bamm:characteristic :Characteristic10;
    bamm:preferredName "Density"@en;
    bamm:description "The product density."@en;
    bamm:exampleValue "3"^^xsd:double.
:Characteristic10 a bamm:Characteristic;
    bamm:name "Characteristic10";
    bamm:dataType xsd:double.
:odor a bamm:Property;
    bamm:name "odor";
    bamm:characteristic :Characteristic11;
    bamm:preferredName "Odor"@en;
    bamm:description "The product odor."@en;
    bamm:exampleValue "Übelriechend, süsslich".
:Characteristic11 a bamm:Characteristic;
    bamm:name "Characteristic11";
    bamm:dataType xsd:string.
:phValue a bamm:Property;
    bamm:name "phValue";
    bamm:characteristic :Characteristic12;
    bamm:preferredName "Ph-Value"@en;
    bamm:description "The Ph-value for liquids."@en;
    bamm:exampleValue "5.5"^^xsd:double.
:Characteristic12 a bamm:Characteristic;
    bamm:name "Characteristic12";
    bamm:dataType xsd:double.
:hazardousness a bamm:Property;
    bamm:name "hazardousness";
    bamm:characteristic :Characteristic13;
    bamm:preferredName "Hazardousness"@en;
    bamm:description "If the product can be classified as hazardous"@en;
    bamm:exampleValue "yes / no"^^xsd:boolean.
:Characteristic13 a bamm:Characteristic;
    bamm:name "Characteristic13";
    bamm:dataType xsd:boolean.
:packagingType a bamm:Property;
    bamm:name "packagingType";
    bamm:characteristic :Characteristic14;
    bamm:preferredName "Packaging Type"@en;
    bamm:description "Packaging type for the product"@en;
    bamm:exampleValue "Container, bins, drum, IBC".
:Characteristic14 a bamm:Characteristic;
    bamm:name "Characteristic14";
    bamm:dataType xsd:string.
:Constraint1 a bamm:Constraint;
    bamm:name "Constraint1".
:Wastecode a bamm:Entity;
    bamm:name "Wastecode";
    bamm:properties ().
