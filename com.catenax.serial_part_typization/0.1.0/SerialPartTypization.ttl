@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:1.0.0#>.
@prefix unit: <urn:bamm:io.openmanufacturing:unit:1.0.0#>.
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:1.0.0#>.
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:1.0.0#>.
@prefix : <urn:bamm:com.catenax.serial_part_typization:0.1.0#>.

:ProductionCountryCodeTrait a bamm-c:Trait;
    bamm:name "ProductionCountryCodeTrait";
    bamm:preferredName "Production Country Code Trait"@en;
    bamm:description "Trait to ensure standard data format for country code"@en;
    bamm-c:constraint :CountryCodeConstraint;
    bamm-c:baseCharacteristic :CountryCodeCharacteristic.
:CountryCodeConstraint a bamm-c:LengthConstraint;
    bamm:name "CountryCodeConstraint";
    bamm:description "Ensures the ISO three-letter code for countries"@en;
    bamm-c:minValue "3"^^xsd:nonNegativeInteger;
    bamm-c:maxValue "3"^^xsd:nonNegativeInteger.
:CountryCodeCharacteristic a bamm:Characteristic;
    bamm:name "CountryCodeCharacteristic";
    bamm:preferredName "Country Code Characteristic"@en;
    bamm:description "ISO 3166-1 alpha-3 – three-letter country codes "@en;
    bamm:dataType xsd:string;
    bamm:see <https%25253A%25252F%25252Fwww.iso.org%25252Fiso-3166-country-codes.html>.
:UUIDv4RegularExpression a bamm-c:RegularExpressionConstraint;
    bamm:name "UUIDv4RegularExpression";
    bamm:preferredName "Catena-X UniqueId Regular Expression"@en;
    bamm:description "Regular Expression for UUID v4"@en;
    bamm:value "^[0-9a-fA-F]{8}\\\\\\\\b-[0-9a-fA-F]{4}\\\\\\\\b-[0-9a-fA-F]{4}\\\\\\\\b-[0-9a-fA-F]{4}\\\\\\\\b-[0-9a-fA-F]{12}$".
:UUIDv4 a bamm:Characteristic;
    bamm:name "UUIDv4";
    bamm:preferredName "UUIDv4"@en;
    bamm:description "A Version 4 UUID is a universally unique identifier that is generated using random numbers. "@en;
    bamm:dataType xsd:string.
:KeyValueList a bamm:Entity;
    bamm:name "KeyValueList";
    bamm:preferredName "Key Value List"@en;
    bamm:description "A list of key values pairs, which compose the local identifier"@en;
    bamm:properties (:key :value).
:key a bamm:Property;
    bamm:name "key";
    bamm:preferredName "Identifier Key"@en;
    bamm:description "The key (domain, namespace) component of an identifier."@en;
    bamm:characteristic bamm-c:Text;
    bamm:exampleValue "com.samplecompany.serialnumber".
:value a bamm:Property;
    bamm:name "value";
    bamm:preferredName "Identifier Value"@en;
    bamm:description "The value of an identifier."@en;
    bamm:characteristic bamm-c:Text;
    bamm:exampleValue "SN12345678".
:SerialPartTypization a bamm:Aspect;
    bamm:name "SerialPartTypization";
    bamm:preferredName "Serial Part Typization"@en;
    bamm:description "Serialized parts are instantiations of parts where the particular instantiation can be uniquely identified by means of serial numbers or similar identifiers (e.g. VIN). "@en;
    bamm:properties (:catenaXId :localIdentifier [
  bamm:property :name;
  bamm:optional "true"^^xsd:boolean
] :production :type [
  bamm:property :shortDescription;
  bamm:optional "true"^^xsd:boolean
] [
  bamm:property :vendor;
  bamm:optional "true"^^xsd:boolean
] [
  bamm:property :partNumber;
  bamm:optional "true"^^xsd:boolean
] [
  bamm:property :weight;
  bamm:optional "true"^^xsd:boolean
] [
  bamm:property :mileage;
  bamm:optional "true"^^xsd:boolean
]);
    bamm:operations ().
:catenaXId a bamm:Property;
    bamm:name "catenaXId";
    bamm:preferredName "Catena-X Identifier"@en;
    bamm:description "The fully anonymous Catena-X ID of the serialized part, valid for the Catena-X dataspace."@en;
    bamm:characteristic :CatenaXIdTrait;
    bamm:exampleValue "580d3adf-1981-44a0-a214-13d6ceed9379".
:localIdentifier a bamm:Property;
    bamm:name "localIdentifier";
    bamm:preferredName "Local Identifier"@en;
    bamm:description "The local identifier, which uniquely identifies a part in specific dataspaces, but is not unique in Catena-X dataspace "@en;
    bamm:characteristic :LocalIdentifierCharacteristic.
:name a bamm:Property;
    bamm:name "name";
    bamm:preferredName "Name"@en;
    bamm:description "Name of the serialized part"@en;
    bamm:characteristic :NameCharacteristic;
    bamm:exampleValue "Mirror left"^^:NameEntity.
:production a bamm:Property;
    bamm:name "production";
    bamm:preferredName "Production"@en;
    bamm:description "data from production process, such as production date and production country"@en;
    bamm:characteristic :ProductionCharacteristic.
:type a bamm:Property;
    bamm:name "type";
    bamm:preferredName "Type"@en;
    bamm:description "The type of part"@en;
    bamm:characteristic :TypeCharacteristic;
    bamm:exampleValue "Software".
:shortDescription a bamm:Property;
    bamm:name "shortDescription";
    bamm:preferredName "Short Description"@en;
    bamm:description "A short description of the part"@en;
    bamm:characteristic :ShortDescriptionCharacteristic.
:LocalIdentifierCharacteristic a bamm:Characteristic;
    bamm:name "LocalIdentifierCharacteristic";
    bamm:preferredName "Local Identifier Characteristic"@en;
    bamm:description "One part can have multiple attributes, the uniquely identify a serialized part in the  dataspace of the manufacturer/producer, e.g. production site + serial number"@en;
    bamm:dataType :KeyValueList.
:ProductionCharacteristic a bamm:Characteristic;
    bamm:name "ProductionCharacteristic";
    bamm:preferredName "Production Characteristic"@en;
    bamm:description "Characteristic to describe production related data"@en;
    bamm:dataType :ProductionEntity.
:ProductionEntity a bamm:Entity;
    bamm:name "ProductionEntity";
    bamm:preferredName "Production Entity"@en;
    bamm:description "This entity capsulates the production relevant attributes"@en;
    bamm:properties (:date [
  bamm:property :country;
  bamm:optional "true"^^xsd:boolean
]).
:date a bamm:Property;
    bamm:name "date";
    bamm:preferredName "Production Date"@en;
    bamm:description "Timestamp, when the part was produced"@en;
    bamm:characteristic bamm-c:Timestamp;
    bamm:exampleValue "2022-02-04T14:48:54"^^xsd:dateTime.
:country a bamm:Property;
    bamm:name "country";
    bamm:preferredName "Country code"@en;
    bamm:description "Country code where the part was produced"@en;
    bamm:characteristic :ProductionCountryCodeTrait;
    bamm:exampleValue "HUR".
:TypeCharacteristic a bamm-c:Enumeration;
    bamm:name "TypeCharacteristic";
    bamm:preferredName "Type Characteristic"@en;
    bamm:description "An serialized Part must be placed into one of the following categories:\n'component', 'product', 'software', ‘assembly’, 'tool', or 'raw material'."@en;
    bamm:dataType xsd:string;
    bamm-c:values ("Product" "Software" "Assembly" "Tool" "Raw Material" "Component").
:ShortDescriptionCharacteristic a bamm:Characteristic;
    bamm:name "ShortDescriptionCharacteristic";
    bamm:preferredName "ShortDescriptionCharacteristic"@en;
    bamm:description "The short description has a max length of 254 characters. "@en;
    bamm:dataType xsd:string.
:NameCharacteristic a bamm:Characteristic;
    bamm:name "NameCharacteristic";
    bamm:preferredName "Name Characterisic"@en;
    bamm:dataType :NameEntity.
:NameEntity a bamm:Entity;
    bamm:name "NameEntity";
    bamm:preferredName "Name Entity"@en;
    bamm:properties (:nameAtManufacturer :nameAtCustomer).
:nameAtManufacturer a bamm:Property;
    bamm:name "nameAtManufacturer";
    bamm:preferredName "Name At Manufacturer"@en;
    bamm:description "Name of the part, assigned by the manufacturer"@en;
    bamm:characteristic :NameAtManufacturerCharacteristic;
    bamm:exampleValue "Mirror left".
:nameAtCustomer a bamm:Property;
    bamm:name "nameAtCustomer";
    bamm:preferredName "name At Customer"@en;
    bamm:description "Name of the part, as assigned by the customer"@en;
    bamm:characteristic :NameAtCustomerCharacteristic;
    bamm:exampleValue "Mirror Links".
:vendor a bamm:Property;
    bamm:name "vendor";
    bamm:preferredName "Vendor"@en;
    bamm:description "The manufacturer of the part"@en;
    bamm:characteristic :VendorCharacteristic.
:VendorCharacteristic a bamm:Characteristic;
    bamm:name "VendorCharacteristic";
    bamm:preferredName "Vendor Characteristic"@en;
    bamm:dataType xsd:string.
:NameAtManufacturerCharacteristic a bamm:Characteristic;
    bamm:name "NameAtManufacturerCharacteristic";
    bamm:preferredName "Name at Manufacturer Characteristic"@en;
    bamm:dataType xsd:string.
:NameAtCustomerCharacteristic a bamm:Characteristic;
    bamm:name "NameAtCustomerCharacteristic";
    bamm:preferredName "Name at Customer Characteristic"@en;
    bamm:dataType xsd:string.
:partNumber a bamm:Property;
    bamm:name "partNumber";
    bamm:preferredName "Part Number"@en;
    bamm:description "The part number of the part design"@en;
    bamm:characteristic :PartNumberCharacteristic.
:PartNumberCharacteristic a bamm:Characteristic;
    bamm:name "PartNumberCharacteristic";
    bamm:preferredName "Part Number Characteristic"@en;
    bamm:dataType :PartNumberEntity.
:partNumberAtManufacturer a bamm:Property;
    bamm:name "partNumberAtManufacturer";
    bamm:preferredName "Part Number at Manufacturer"@en;
    bamm:description "part number assigned by the manufacturer to identify the part design"@en;
    bamm:characteristic :PartNumberAtManufacturerCharacteristic;
    bamm:exampleValue "123-0.740-3434-A".
:partNumberAtCustomer a bamm:Property;
    bamm:name "partNumberAtCustomer";
    bamm:preferredName "Part Number at Customer"@en;
    bamm:description "part number (assigned by the customer) to identify the part design"@en;
    bamm:characteristic :PartNameAtCustomerCharacteristic;
    bamm:exampleValue "PRT-12345".
:PartNumberAtManufacturerCharacteristic a bamm:Characteristic;
    bamm:name "PartNumberAtManufacturerCharacteristic";
    bamm:preferredName "Part Number At Manufacturer Characteristic"@en;
    bamm:dataType xsd:string.
:PartNumberEntity a bamm:Entity;
    bamm:name "PartNumberEntity";
    bamm:preferredName "Part Number Entity"@en;
    bamm:description "Encapsulation for the various part numbers"@en;
    bamm:properties (:partNumberAtManufacturer :partNumberAtCustomer).
:PartNameAtCustomerCharacteristic a bamm:Characteristic;
    bamm:name "PartNameAtCustomerCharacteristic";
    bamm:preferredName "Part Name At Customer Characteristic"@en;
    bamm:dataType xsd:string.
:weight a bamm:Property;
    bamm:name "weight";
    bamm:preferredName "weight"@en;
    bamm:description "weight of the serialized part in kg"@en;
    bamm:characteristic :WeightCharacteristic;
    bamm:exampleValue "151"^^xsd:float.
:mileage a bamm:Property;
    bamm:name "mileage";
    bamm:preferredName "Mileage"@en;
    bamm:description "cummulated mileage of the serialized part (including second life) in km"@en;
    bamm:characteristic :MileageCharacteristic.
:WeightCharacteristic a bamm:Characteristic;
    bamm:name "WeightCharacteristic";
    bamm:preferredName "Weight Characteristic"@en;
    bamm:dataType xsd:float.
:MileageCharacteristic a bamm:Characteristic;
    bamm:name "MileageCharacteristic";
    bamm:preferredName "Mileage Characteristic"@en;
    bamm:dataType xsd:float.
:CatenaXIdTrait a bamm-c:Trait;
    bamm:name "CatenaXIdTrait";
    bamm:preferredName "Catena-X ID Trait"@en;
    bamm:description "Trait to ensure data format for Catena-X ID"@en;
    bamm-c:constraint :UUIDv4RegularExpression;
    bamm-c:baseCharacteristic :UUIDv4.
